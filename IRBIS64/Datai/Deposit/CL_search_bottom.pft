        fi        
  else
        if val(v1002)>val(v1006)    then
                if (val(v1008^A)=1) then else '<a href="IRBIS:?I21DBN=',&unifor('Av1000#1'),'&C21COM=2','&PFTNAME=',&uf('Av1012#1'),'&S21ALL=',&unifor('+3E',&unifor('Av1007#1')),'&QUERY=',&unifor('+3E',&unifor('Av1010#1')),'&S21CNR=',&unifor('Av1006#1'),'&S21STN=1&S21SRD=&S21SRW=&BOTTOMPFTNAME=CL_search_bottom&TOPPFTNAME=CL_search_bottom&S21ALLTRM=',&unifor('+3E',&unifor('Av1009#1')),'">' fi,
                '1',
