/* g10 - результат контроля: если пусто - ДА
&uf('+7W10#'),
if (v3:'**') and (&uf('+95',v2)<>&uf('+95',v3)) then &uf('+7W10#1') fi /*1 несовпадение длины шаблона и счетчика при **,
if g10='' then 
    &uf('+7W3#',&uf('G0*',v3)),
    /* g3 - префиксная часть шаблона 
    if (g3<>'') and (&uf('+9S!',g3,'!',v2)<>'1') then &uf('+7W10#2') fi /*2 несовпадение префиксной части
fi 
if g10='' then 
    &uf('+7W4#',&uf('G1*',v3)),/* g4 - часть шаблона без префиксной части
    (if &uf('+960.1#',&uf('Ag4#1'))='*' then &uf('+7W4#',&uf('+960*1.100#',&uf('Ag4#1'))) else break fi),
    /* g4 - суффиксная часть шаблона
    &uf('+7W5#',&uf('+95',g4))
    /* g5 - длина суффиксной части шаблона          
    if (g4<>'') and (&uf('+961.',g5,'#',v2)<>g4) then &uf('+7W10#3') fi /*3 несовпадение суффиксной части
fi 
if g10='' then 
    &uf('+7W6#',&uf('+95',g3)), /* g6 - длина префиксной части шаблона
    &uf('+7W7#',&uf('+960*',g6,'.100#',v2)),&uf('+7W7#',&uf('+961*',g5,'.100#',g7)), /* g7 - числовая часть счетчика
    (if '0123456789':&uf('+960.1#',&uf('Ag7#1')) then &uf('+7W7#',&uf('+960*1.100#',&uf('Ag7#1'))) else &uf('+7W10#4'),break fi), /* проверка на цифры
fi         
         
